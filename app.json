{
  "expo": {
    "name": "Santander",
    "slug": "santa-shaco",
    "version": "1.0.0",
    "web": {
      "favicon": "./assets/splash/bootsplash_logo.png"
    },
    "experiments": {
      "tsconfigPaths": true
    },
    "plugins": [],
    "orientation": "portrait",
    "icon": "./assets/splash/bootsplash_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash/bootsplash_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hlspablo.santa-shaco"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash/bootsplash_logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hlspablo.santashaco"
    }
  }
}
